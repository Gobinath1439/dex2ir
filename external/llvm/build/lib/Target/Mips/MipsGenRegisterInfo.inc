      0,	// sub_32_sub_hi_then_sub_32
    },
    {	// LORegs64
      27,	// sub_32 -> LORegs64
      0,	// sub_dsp16_19
      0,	// sub_dsp20
      0,	// sub_dsp21
      0,	// sub_dsp22
      0,	// sub_dsp23
      0,	// sub_fpeven
      0,	// sub_fpodd
      0,	// sub_hi
      0,	// sub_lo
      0,	// sub_hi_then_sub_32
      0,	// sub_32_sub_hi_then_sub_32
    },
    {	// ACRegs128
      28,	// sub_32 -> ACRegs128
      0,	// sub_dsp16_19
      0,	// sub_dsp20
      0,	// sub_dsp21
      0,	// sub_dsp22
      0,	// sub_dsp23
      0,	// sub_fpeven
      0,	// sub_fpodd
      28,	// sub_hi -> ACRegs128
      28,	// sub_lo -> ACRegs128
      28,	// sub_hi_then_sub_32 -> ACRegs128
      28,	// sub_32_sub_hi_then_sub_32 -> ACRegs128
    },
  };
  assert(RC && "Missing regclass");
  if (!Idx) return RC;
  --Idx;
  assert(Idx < 12 && "Bad subreg");
  unsigned TV = Table[RC->getID()][Idx];
  return TV ? getRegClass(TV - 1) : 0;
}

