virtual bool CheckPatternPredicate(unsigned PredNo) const {
  switch (PredNo) {
  default: llvm_unreachable("Invalid predicate in table?");
  case 0: return (Subtarget->hasAVX());
  case 1: return (Subtarget->hasSSE1() && !Subtarget->hasAVX());
  case 2: return (Subtarget->hasSSE2() && !Subtarget->hasAVX());
  case 3: return (Subtarget->hasSSE2());
  case 4: return (!Subtarget->is64Bit());
  case 5: return (Subtarget->is64Bit());
  case 6: return (Subtarget->hasAVX() && !Subtarget->hasAVX512());
  case 7: return (Subtarget->hasSSE41() && !Subtarget->hasAVX());
  case 8: return (Subtarget->hasAVX512());
  case 9: return (Subtarget->hasAVX() && !Subtarget->hasAVX2());
  case 10: return (Subtarget->hasAVX2());
  case 11: return (TM.getCodeModel() == CodeModel::Small ||TM.getCodeModel() == CodeModel::Kernel) && (TM.getRelocationModel() == Reloc::Static);
  case 12: return (Subtarget->hasMOVBE());
  case 13: return (!Subtarget->hasSSE1());
  case 14: return (!Subtarget->hasSSE2());
  case 15: return (Subtarget->hasMMX());
  case 16: return (Subtarget->hasSSSE3());
  case 17: return (Subtarget->hasBMI());
  case 18: return (Subtarget->hasBMI2());
  case 19: return (Subtarget->hasSSE42());
  case 20: return (Subtarget->hasSSE1());
  case 21: return (Subtarget->has3DNow());
  case 22: return (Subtarget->hasXOP());
  case 23: return (Subtarget->hasSSE42() && !Subtarget->hasAVX());
  case 24: return (Subtarget->hasSSSE3() && !Subtarget->hasAVX());
  case 25: return (Subtarget->hasAVX()) && (Subtarget->hasAES());
  case 26: return (Subtarget->hasAES());
  case 27: return (Subtarget->hasAVX()) && (Subtarget->hasPCLMUL());
  case 28: return (Subtarget->hasPCLMUL());
  case 29: return (Subtarget->hasSSE4A());
  case 30: return (Subtarget->hasAVX()) && (Subtarget->hasF16C());
  case 31: return (Subtarget->hasSSE3() && !Subtarget->hasAVX());
